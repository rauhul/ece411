p_grp:= function(p,n)
local i,k,ii,l,j,gens,t,first,pow,rem;

 gens:=[];
 rem:=n;
 first:=1;
 i:=1;
 while rem>=p do
  k:=1;
  pow:=p;
  while pow*p<=rem do
   k:=k+1;
   pow:=pow*p;
  od;
  rem:=rem-pow;
  j:=1;
  for l in [1..k] do
   t:=[1..p*j];
   for ii in [1..p*j] do
    t[ii]:=((ii+j-1) mod (p*j))+1;
   od;
   gens[i]:=PermList(Concatenation([1..first-1],t+(first-1)));
   i:=i+1;
   j:=j*p;
  od;
  first:=first+pow;
 od;
 return Group(gens,());
end;;


NextVec:=function(v,p,d)
